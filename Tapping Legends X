-- Functions

function aBasicEgg()
spawn(function()
    while getgenv().aBasicEgg == true do
        local args = {
            [1] = "Basic Egg",
            [2] = 1
            }
            game:GetService("ReplicatedStorage").Remotes.BuyEgg:InvokeServer(unpack(args))
        wait(0.2)
    end
end)
end

function aRareEgg()
spawn(function()
    while getgenv().aRareEgg == true do
        local args = {
