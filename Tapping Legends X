-- Tab 3 (Eggs)

local EggsTab = Window:NewTab("Auto Eggs")
local EggsSection = EggsTab:NewSection("Eggs")

EggsSection:NewToggle("Auto Basic Egg", "Auto opens the Basic egg for you", function(state)
getgenv().aBasicEgg = true;
if state then
    local BasicEggLocation = CFrame.new(-55, 157, -796)
    humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    wait(0.1)
    EpicPlayerMan.CFrame = BasicEggLocation
    aBasicEgg()
else
    getgenv().aBasicEgg = false;
end
end)

EggsSection:NewToggle("Auto Basic Egg x3", "Auto opens the Basic egg x3 for you", function(state)
getgenv().aBasicEgg3 = true;
